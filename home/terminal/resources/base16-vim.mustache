" Highlighting function
function! g:Base16hi(group, guifg, guibg, ctermfg, ctermbg, ...)
  let l:attr = get(a:, 1, '')
  let l:guisp = get(a:, 2, '')

  if a:guifg != ''
    if a:guifg ==# 'NONE' || a:guifg ==# 'bg' || a:guifg ==# 'fg'
      exec 'hi ' . a:group . ' guifg=' . a:guifg
    else
      exec 'hi ' . a:group . ' guifg=#' . a:guifg
    endif
  endif

  if a:guibg != ''
    if a:guibg ==# 'NONE' || a:guibg ==# 'bg' || a:guibg ==# 'fg'
      exec 'hi ' . a:group . ' guibg=' . a:guibg
    else
      exec 'hi ' . a:group . ' guibg=#' . a:guibg
    endif
  endif

  if a:ctermfg != ''
    exec 'hi ' . a:group . ' ctermfg=' . a:ctermfg
  endif

  if a:ctermbg != ''
    exec 'hi ' . a:group . ' ctermbg=' . a:ctermbg
  endif

  if l:attr != ''
    exec 'hi ' . a:group . ' gui=' . l:attr . ' cterm=' . l:attr
  endif

  if l:guisp != ''
    if l:guisp ==# 'NONE'
      exec 'hi ' . a:group . ' guisp=' . l:guisp
    else
      exec 'hi ' . a:group . ' guisp=#' . l:guisp
    endif
  endif
endfunction

function! s:hi(group, guifg, guibg, ctermfg, ctermbg, ...)
  call g:Base16hi(a:group, a:guifg, a:guibg, a:ctermfg, a:ctermbg, get(a:, 1, ''), get(a:, 2, ''))
endfunction

